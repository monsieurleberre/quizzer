{
  "presets": [
    "es2015",
    "react"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "plugins": [
    "transform-object-rest-spread",
    [
      "transform-regenerator", { 
        "async": false,
        "asyncGenerators": false,
        "generators": true
      }
    ]
  ]
}
